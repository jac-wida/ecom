ductListScreen} />\n          <Route path='/admin/user/:id/edit' component={UserEditScreen} />\n          <Route path='/' component={HomeScreen} exact />\n        </>\n      </main>\n      <Footer />\n    </Router>\n  );\n};\n\nexport default App;\n","C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\reportWebVitals.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Header.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Footer.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\HomeScreen.js",["177"],"import React, { useEffect } from 'react';\r\nimport { Col, Container, Row } from 'react-bootstrap';\r\nimport Product from '../components/Product';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { listProducts } from '../actions/productActions';\r\nimport Message from '../components/Message';\r\n// import Loader from '../components/Loader';\r\nimport './HomeScreen.css';\r\n\r\nimport Spinnner from '../components/Spinner';\r\nimport Background from '../components/Background';\r\n\r\nconst HomeScreen = () => {\r\n  const dispatch = useDispatch();\r\n  const productList = useSelector((state) => state.productList);\r\n  const { loading, error, products } = productList;\r\n\r\n  useEffect(() => {\r\n    dispatch(listProducts());\r\n  }, [dispatch]);\r\n\r\n  return (\r\n    <>\r\n      <Background />\r\n      <div className='latest'>\r\n        <h1>LATEST PRODUCTS</h1>\r\n        {loading ? (\r\n          <Spinnner />\r\n        ) : error ? (\r\n          <Message variant='danger'>{error}</Message>\r\n        ) : (\r\n          <Row>\r\n            {products.map((product) => (\r\n              <Col\r\n                className='wis'\r\n                key={product._id}\r\n                sm={12}\r\n                md={6}\r\n                lg={4}\r\n                xl={3}\r\n              >\r\n                <Product product={product} />\r\n              </Col>\r\n            ))}\r\n          </Row>\r\n        )}\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default HomeScreen;\r\n","C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Product.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Rating.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\ProductScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\store.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\reducers\\productReducer.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\constants\\productConstants.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\actions\\productActions.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Loader.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\Message.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\CartScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\reducers\\cartReducer.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\constants\\cartConstants.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\actions\\cartActions.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\reducers\\userReducers.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\constants\\userConstants.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\LoginScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\actions\\userActions.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\FormContainer.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\RegisterScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\ProfileScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\ShippingScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\components\\CheckoutSteps.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\PaymentScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\PlaceOrderScreen.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\reducers\\orderreducer.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\constants\\orderConstants.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\actions\\orderActions.js",[],"C:\\Users\\Wulex\\Desktop\\mern\\ecom\\src\\screens\\OrderScreen.js",["178"],"import React, { useState, useEffect } from 'react';\r\nimport { PayPalButton } from 'react-paypal-button-v2';\r\nimport axios from 'axios';\r\nimport { Row, Col, Image, ListGroup, Card } from 'react-bootstrap';\r\nimport { Link } from 'react-router-dom';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport Message from '../components/Message';\r\nimport { getOrderDetails, payOrder } from '../actions/orderActions';\r\nimport { ORDER_PAY_RESET } from '../constants/orderConstants';\r\nimport Spinnner from '../components/Spinner';\r\n\r\nconst OrderScreen = ({ match, history }) => {\r\n  const orderId = match.params.id;\r\n\r\n  const [sdkReady, setSdkReady] = useState(false);\r\n\r\n  const dispatch = useDispatch();\r\n  const orderDetails = useSelector((state) => state.orderDetails);\r\n  const { order, loading, error } = orderDetails;\r\n\r\n  const successPaymentHandler = (paymentResult) => {\r\n    console.log(paymentResult);\r\n\r\n    dispatch(payOrder(orderId, paymentResult));\r\n  };\r\n\r\n  const orderPay = useSelector((state) => state.orderPay);\r\n  const { loading: loadingPay, success: successPay } = orderPay;\r\n\r\n  const userLogin = useSelector((state) => state.userLogin);\r\n  const { userInfo } = userLogin;\r\n\r\n  useEffect(() => {\r\n    if (!userInfo) {\r\n      history.push('/login');\r\n    }\r\n\r\n    const addPayPalScript = async () => {\r\n      const { data: clientId } = await axios.get('/api/config/paypal');\r\n      const script = document.createElement('script');\r\n      script.type = 'text/javascript';\r\n      script.src = `https://www.paypal.com/sdk/js?client-id=${clientId}`;\r\n      script.async = true;\r\n      script.onload = () => {\r\n        setSdkReady(true);\r\n      };\r\n      document.body.appendChild(script);\r\n    };\r\n\r\n    if (!order || successPay) {\r\n      dispatch({ type: ORDER_PAY_RESET });\r\n      dispatch(getOrderDetails(orderId));\r\n    } else if (!order.isPaid) {\r\n      if (!window.paypal) {\r\n        addPayPalScript();\r\n      } else {\r\n        setSdkReady(true);\r\n      }\r\n    }\r\n  }, [dispatch, orderId, order, successPay, history, userInfo]);\r\n\r\n  if (!loading) {\r\n    //   Calculate prices\r\n    const addDecimals = (num) => {\r\n      return (Math.round(num * 100) / 100).toFixed(2);\r\n    };\r\n\r\n    order.itemsPrice = addDecimals(\r\n      order.orderItems.reduce((acc, item) => acc + item.price * item.qty, 0)\r\n    );\r\n  }\r\n\r\n  return loading ? (\r\n    <Spinnner />\r\n  ) : error ? (\r\n    <Message variant='danger'>{error}</Message>\r\n  ) : (\r\n    <>\r\n      <h1>ORDER: {order._id}</h1>\r\n      <Row>\r\n        <Col md={8}>\r\n          <ListGroup variant=''>\r\n            <ListGroup.Item>\r\n              <h2>SHIPPING:</h2>\r\n              <p>\r\n                <strong>Name: {order.user.name}</strong>\r\n              </p>\r\n              <p>\r\n                <strong>\r\n                  Email:{' '}\r\n                  <a href={`mailto: ${order.user.email}`}>{order.user.email}</a>\r\n                </strong>\r\n              </p>\r\n              <p>\r\n                <strong>Address : </strong>\r\n                {order.shippingAddress.address}, {order.shippingAddress.city},\r\n                {order.shippingAddress.postalCode},{' '}\r\n                {order.shippingAddress.country}\r\n              </p>\r\n              {order.isDelivered ? (\r\n                <Message variant='success'>{order.deliveredAt}</Message>\r\n              ) : (\r\n                <Message variant='danger'>Not Delivered</Message>\r\n              )}\r\n            </ListGroup.Item>\r\n\r\n            <ListGroup.Item>\r\n              <h2>PAYMENT METHOD</h2>\r\n              {/* <strong>Method: </strong> */}\r\n              <p>{order.paymentMethod}</p>\r\n    